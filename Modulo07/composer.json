{
    "name": "blader250/buscadora-de-cursos",
    "description": "Esse biblioteca busca cursos na alura",
    "type": "library",
    "require": {
        "guzzlehttp/guzzle": "^7.3",
        "symfony/dom-crawler": "^5.2",
        "symfony/css-selector": "^5.2"
    },
    "autoload": {
        "psr-4": {
            "Alura\\Buscador\\":"src/"
        }
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.6"
    },
    "scripts": {
        "cs": "phpcs --standard=PSR12 src/"
    },
    "scripts-descriptions": {
        "cs": "Busca por erros com base na PSR12"
    }
}